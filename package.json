{
  "name": "sass-setup",
  "version": "1.0.0",
  "description": "Basic sass setup to compile scss file into css and integrate in your webpage",
  "main": "index.js",
  "scripts": {
    "sass-dev": "sass --watch --update --style=expanded assets/scss:assets/css",
    "sass-prod": "sass --no-source-map --style=compressed assets/scss:assets/css"
  },
  "keywords": [
    "sass-setup"
  ],
  "author": "sepatil",
  "license": "ISC",
  "devDependencies": {
    "sass": "^1.52.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "bootstrap": "^5.1.3"
  }
}
